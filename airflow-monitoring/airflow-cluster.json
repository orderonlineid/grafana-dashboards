          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "select\n        -- Se último erro ocorreu com intervalo menor ou igual a 1 minuto, e em menos de 10 minutos\n        case when dif_last_error <= '00:01:00' and dif_now < '00:10:00' then\n        'Stable'\n        -- Se útimo erro ocorreu com intervalo superior a 1 minuto, e em menos de 30 minutos\n        when dif_last_error > '00:01:00' and dif_now < '00:30:00' then\n        'Warning'\n        -- Se o percentual de erros estiver entre 5 a 10\n        when per_error between 5 and 10 then\n        'Warning'\n        -- Se o percentual de erros for superior a 10 minutos\n        when per_error > 10 then\n        'Stable'\n        else\n        'Estável'\n        end as status_airflow\nfrom\n(\n    select\n        to_char(last_failed - before_last_failed, 'HH24:MI:SS')::time as dif_last_error,\n        to_char(current_timestamp - last_failed, 'HH24:MI:SS')::time as dif_now,\n        (qtd_error::float /\n        (\n            select\n                count(*)\n            from\n                dag_run dr3\n            where\n                    date(dr3.execution_date) = current_date\n        )::float) * 100 as per_error\n    from\n        (\n        select\n            (\n              select\n                      max(execution_date)\n              from\n                        dag_run dr2\n              where\n                      date(dr2.execution_date) = current_date and\n                      dr2.state = 'failed' and\n                      dr2.execution_date < max(dr1.execution_date)\n            ) as before_last_failed,\n            max(execution_date) as last_failed,\n            count(*) as qtd_error\n        from\n            dag_run dr1\n        where\n            date(execution_date) = current_date and\n            state = 'failed'\n    ) as errors\n) as gr",
          "refId": "A",
          "select": [
            [
              {
                "params": [
                  "duration"
                ],
                "type": "column"
              }
            ]
          ],
          "table": "task_instance",
          "timeColumn": "start_date",
          "timeColumnType": "timestamp",
          "where": [
            {
              "name": "$__timeFilter",
              "params": [],
              "type": "macro"
            }
          ]
        }
      ],
      "title": "Status Airflow",
      "type": "stat"
